{
  "name": "spur-super-app",
  "version": "0.1.0",
  "description": "Spur Super App - Always-on personal productivity companion",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:extension\" \"npm run dev:web\"",
    "dev:extension": "vite build --config vite.extension.config.js --watch",
    "dev:web": "vite --config vite.web.config.js",
    "build": "npm run build:extension && npm run build:web",
    "build:extension": "vite build --config vite.extension.config.js",
    "build:web": "vite build --config vite.web.config.js",
    "test": "vitest",
    "test:ui": "playwright test",
    "test:e2e": "playwright test --config=e2e.config.js",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist dist-extension dist-web",
    "package": "npm run build && cd dist-extension && zip -r ../spur-extension.zip .",
    "setup": "npm install && npm run build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "zustand": "^4.4.7",
    "@tanstack/react-query": "^5.8.4",
    "sqlite3": "^5.1.6",
    "dexie": "^3.2.4",
    "rxdb": "^15.0.0",
    "rxjs": "^7.8.1",
    "winston": "^3.11.0",
    "compromise": "^14.11.0",
    "natural": "^6.12.0",
    "onnxruntime-web": "^1.16.3",
    "@tensorflow/tfjs": "^4.22.0",
    "webextension-polyfill": "^0.11.0",
    "chokidar": "^3.5.3",
    "crypto-js": "^4.2.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "axios": "^1.6.2",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.295.0",
    "tailwindcss": "^3.3.6",
    "clsx": "^2.0.0",
    "react-hot-toast": "^2.4.1",
    "react-hotkeys-hook": "^4.4.1",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "recharts": "^2.8.0",
    "d3": "^7.8.5",
    "@types/d3": "^7.4.3",
    "dayjs": "^1.11.10",
    "fuse.js": "^6.6.2",
    "uuid": "^9.0.1",
    "@types/uuid": "^9.0.7",
    "workbox-background-sync": "^7.0.0",
    "workbox-broadcast-update": "^7.0.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-google-analytics": "^7.0.0",
    "workbox-navigation-preload": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-range-requests": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "workbox-streams": "^7.0.0",
    "workbox-window": "^7.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/node": "^20.10.4",
    "@types/chrome": "^0.0.251",
    "@types/webextension-polyfill": "^0.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/crypto-js": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "typescript": "^5.3.3",
    "concurrently": "^8.2.2",
    "rimraf": "^5.0.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "tailwindcss-animate": "^1.0.7",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "playwright": "^1.40.0",
