    "personal-assistant",
    "context-aware",
    "privacy-first",
    "local-first",
    "react",
    "typescript",
    "pwa",
    "super-app"
  ],
  "author": "Spur Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spur/super-app"
  },
  "bugs": {
    "url": "https://github.com/spur/super-app/issues"
  },
  "homepage": "https://spur.com",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions"
  ]
}